{
    "name": "P to P",
    "description": "You can copy all the images on the page you have open in Pintrest to the clipboard.You can paste them into Pureref.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16":"icons/16.png",
        "32":"icons/32.png",
        "48":"icons/48.png",
        "128":"icons/128.png"
    },
    "action": {
        "default_popup":"popup.html"
    },
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["http://*/*", "https://*/*"],
    "background":{}
}